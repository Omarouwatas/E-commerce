{
  "extends": "expo/tsconfig.base",

  "compilerOptions": {
    "jsx": "react",
    "typeRoots": ["./types", "./node_modules/@types"],

    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
